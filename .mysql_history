Describe Details;
SELECT AcctID FROM Accounts, Details LEFT JOIN on Accounts.AcctID=Details.DId WHERE AcctEntityType='Patner';
SELECT AcctID FROM Accounts, Details LEFT JOIN on Accounts.AcctID=Details.DId WHERE AcctEntityType='Partner Organization' AND AcctCreatedDT > '2017-09-01';
SELECT AcctID FROM Accounts, Details LEFT JOIN on Accounts.AcctID=Details.DId WHERE AcctEntityType='Partner Organization' and  AcctCreatedDT > '2017-09-01';
SELECT AcctId FROM Accounts join Details as OrgId on Accounts.AcctId=Details.DOrgId WHERE AcctEntityType='Partner Organization' and AcctCreatedDT > '2017-09-01';
SELECT AcctID FROM Accounts, Details LEFT JOIN on Accounts.AcctID=Details.DOId WHERE AcctEntityType='Patner';
SELECT AcctId FROM Accounts join Details as OrgId on Accounts.AcctId=Details.DOId WHERE AcctEntityType='Partner Organization' and AcctCreatedDT > '2017-09-01';
SELECT AcctId FROM Accounts join Details as OrgId on Accounts.AcctId=Details.DId WHERE AcctEntityType='Partner Organization' and AcctCreatedDT > '2017-09-01';
SELECT AcctId FROM Accounts join Details as OId on Accounts.AcctId=Details.DId WHERE AcctEntityType='Partner Organization' and AcctCreatedDT > '2017-09-01';
